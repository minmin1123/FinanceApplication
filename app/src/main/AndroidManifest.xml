<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.application.example.financeapplication">

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.CountActivity">
        </activity>
        <activity android:name=".activity.CheckActivity">
        </activity>
        <activity android:name=".activity.ManageActivity">
        </activity>
        <activity android:name=".activity.DayItemSolutionActivity">
        </activity>
        <activity android:name=".activity.MonthItemSolutionActivity">
        </activity>
    </application>

</manifest>